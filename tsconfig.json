{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "outDir": "dist",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@lib/*": ["./lib/*"],
      "@services/*": ["./services/*"],
      "@utils/*": ["./utils/*"]
    }
  },
  "include": [
    "./api/*.ts",
    "./*.js",
    "src/**/*.ts",
    "test/**/*.ts"
, "utils/common.ts"  ]
}
