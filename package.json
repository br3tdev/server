{
  "name": "server",
  "version": "2.0.0",
  "license": "MIT",
  "keywords": [],
  "main": "src/index.ts",
  "scripts": {
    "start": "node --env-file=.env dist/src/index.js",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "lint": "eslint --fix src test",
    "test": "vitest run",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:studio": "prisma studio",
    "typecheck": "tsc --noEmit",
    "vercel-build": "pnpm db:generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "ussd-builder": "^1.3.2",
    "ussd-menu-builder": "^1.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/lodash": "^4.17.21",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.9.2",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.38.0",
    "eslint-plugin-format": "^1.0.2",
    "prisma": "^7.1.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "_moduleAliases": {
    "@lib": "./lib/*",
    "@services": "./services/*",
    "@utils": "./utils/*",
    "@/*": "./src/*"
  }
}
